{
  "manifest_version" : 3,
  "name" : "Pixiv Helper",
  "description" : "Toolkit for downloading images from www.pixiv.net",
  "version" : "0.0.1",
  "default_locale" : "zh_CN",
  "author" : "1980114953@qq.com",
  "icons" : {
    "16" : "images/icon-16.png",
    "32" : "images/icon-32.png",
    "48" : "images/icon-48.png",
    "128" : "images/icon-128.png"
  },
  "permissions" : [
    "storage",
    "activeTab",
    "scripting"
  ]
}